{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"outDir": "./dist/out-tsc",
		"resolveJsonModule": true,
		"sourceMap": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"sourceRoot": "/",
		"inlineSources": true,
		"declaration": false,
		"module": "es2022",
		"moduleResolution": "bundler",
		"emitDecoratorMetadata": false,
		"experimentalDecorators": true,
		"target": "es2022",
		"typeRoots": ["node_modules/@types", "src/types"],
		"lib": ["es2024", "dom"],
		"paths": {
			"@core/*": ["src/app/core/*"],
			"@apps/*": ["src/app/apps/*"],
			"@shares/*": ["src/app/shares/*"],
			"@shared/*": ["src/app/shared/*"],
			"@fileBrowser/*": ["src/app/file-browser/*"],
			"@auth/*": ["src/app/auth/*"],
			"@pledge/*": ["src/app/pledge/*"],
			"@public/*": ["src/app/public/*"],
			"@share-preview/*": ["src/app/share-preview/*"],
			"@embed/*": ["src/app/embed/*"],
			"@search/*": ["src/app/search/*"],
			"@search": ["src/app/search"],
			"@models/*": ["src/app/models/*"],
			"@models": ["src/app/models"],
			"@root/*": ["src/*"],
			"@onboarding/*": ["src/app/onboarding/*"],
			"@announcement/*": ["src/app/announcement/*"]
		},
		"useDefineForClassFields": false
	},
	"angularCompilerOptions": {
		"strictTemplates": false
	},
	"exclude": ["node_modules"]
}
