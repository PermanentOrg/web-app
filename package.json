{
  "name": "mdot",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "dev": "ng serve --host local.permanent.org --disable-host-check --configuration=hmr",
    "dev_proxy": "ng serve --host local.permanent.org --disable-host-check --configuration=proxy_hmr",
    "dev_local": "ng serve --base-href /",
    "build": "ng build --base-href /m/ --aot --build-optimizer --prod",
    "build_local": "ng build --base-href /m/ --aot --build-optimizer --configuration=local",
    "build_dev": "ng build --base-href /m/ --aot --build-optimizer --configuration=development",
    "build_staging": "ng build --base-href /m/ --aot --build-optimizer --configuration=staging",
    "test": "ng test --source-map=false",
    "test_build": "ng test --source-map=false --watch=false --browsers chrome_headless_no_sandbox",
    "lint": "ng lint",
    "e2e_local": "protractor e2e/protractor.conf.js",
    "e2e_dev": "protractor e2e/protractor.dev.conf.js",
    "e2e_proxy": "ng e2e -c proxy --protractor-config e2e/protractor.local_build.conf.js --host local.permanent.org"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.2",
    "@angular/common": "^6.1.2",
    "@angular/compiler": "^6.1.2",
    "@angular/core": "^6.1.2",
    "@angular/forms": "^6.1.2",
    "@angular/http": "^6.1.2",
    "@angular/platform-browser": "^6.1.2",
    "@angular/platform-browser-dynamic": "^6.1.2",
    "@angular/router": "^6.1.2",
    "@types/hammerjs": "^2.0.35",
    "@types/lodash": "^4.14.116",
    "binaryjs": "^0.2.1",
    "core-js": "^2.5.4",
    "gsap": "^2.0.1",
    "hammerjs": "^2.0.8",
    "ios-inner-height": "^1.1.0",
    "lodash": "^4.17.10",
    "ngx-cookie-service": "^1.0.10",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "~6.0.8",
    "@angular/compiler-cli": "^6.1.2",
    "@angular/language-service": "^6.1.2",
    "@angularclass/hmr": "^2.1.3",
    "@types/gsap": "^1.19.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.10.25",
    "codelyzer": "~4.2.1",
    "jasmine-bamboo-reporter": "0.0.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
