{
  "name": "mdot",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "dev": "ng serve --host local.permanent.org --disable-host-check --configuration=hmr",
    "dev_proxy": "ng serve --host local.permanent.org --disable-host-check --configuration=proxy_hmr",
    "dev_local": "ng serve --base-href /",
    "build": "ng build --base-href /m/ --aot --build-optimizer --prod",
    "build_local": "ng build --base-href /m/ --aot --configuration=local",
    "build_dev": "ng build --base-href /m/ --aot --build-optimizer --configuration=development",
    "build_staging": "ng build --base-href /m/ --aot --build-optimizer --configuration=staging",
    "test": "ng test --source-map=false",
    "test_build": "ng test --source-map=false --watch=false --browsers chrome_headless_no_sandbox",
    "lint": "ng lint",
    "e2e_local": "protractor e2e/protractor.conf.js",
    "e2e_dev": "protractor e2e/protractor.dev.conf.js",
    "e2e_proxy": "ng e2e -c proxy --protractor-config e2e/protractor.local_build.conf.js --host local.permanent.org"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.10",
    "@angular/common": "^7.2.10",
    "@angular/compiler": "^7.2.10",
    "@angular/core": "^7.2.10",
    "@angular/fire": "^5.1.1",
    "@angular/forms": "^7.2.10",
    "@angular/http": "^7.2.10",
    "@angular/platform-browser": "^7.2.10",
    "@angular/platform-browser-dynamic": "^7.2.10",
    "@angular/router": "^7.2.10",
    "@types/hammerjs": "^2.0.36",
    "@types/lodash": "^4.14.119",
    "binaryjs": "^0.2.1",
    "core-js": "^2.6.2",
    "countup.js-angular2": "^6.1.1",
    "firebase": "^5.8.0",
    "gsap": "^2.0.2",
    "hammerjs": "^2.0.8",
    "ios-inner-height": "^1.1.0",
    "lodash": "^4.17.11",
    "ngx-cookie-service": "^1.0.10",
    "progressbar.js": "^1.0.1",
    "rxjs": "6.4.0",
    "time-ago-pipe": "^1.3.2",
    "ts-key-enum": "^2.0.0",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.0",
    "@angular/cli": "~7.3.6",
    "@angular/compiler-cli": "^7.2.10",
    "@angular/language-service": "^7.2.10",
    "@angularclass/hmr": "^2.1.3",
    "@types/gsap": "^1.20.0",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "^2.0.6",
    "@types/mandrill-api": "^1.0.30",
    "@types/node": "^8.10.39",
    "codelyzer": "~4.2.1",
    "jasmine-bamboo-reporter": "0.0.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "3.2.4"
  }
}
