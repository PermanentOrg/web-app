{
  "name": "mdot",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "dev": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --host local.permanent.org --disable-host-check --configuration=hmr",
    "dev_proxy": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --host local.permanent.org --disable-host-check --configuration=proxy_hmr",
    "dev_local": "ng serve --base-href /",
    "build": "ng build --aot --build-optimizer --prod",
    "build_local": "ng build --aot --configuration=local",
    "build_dev": "ng build --aot --build-optimizer --configuration=development",
    "build_staging": "ng build --aot --build-optimizer --configuration=staging",
    "test": "ng test --source-map=false",
    "test_build": "ng test --source-map=false --watch=false --browsers chrome_headless_no_sandbox",
    "lint": "ng lint",
    "e2e_local": "protractor e2e/protractor.conf.js",
    "e2e_dev": "protractor e2e/protractor.dev.conf.js",
    "e2e_proxy": "ng e2e -c proxy --protractor-config e2e/protractor.local_build.conf.js --host local.permanent.org"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/fire": "^5.3.0",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ng-bootstrap/ng-bootstrap": "^5.2.0",
    "@permanent.org/vis-timeline": "^1.0.1",
    "@types/hammerjs": "^2.0.36",
    "@types/lodash": "^4.14.149",
    "binaryjs": "^0.2.1",
    "core-js": "^3.6.4",
    "countup.js-angular2": "^7.3.0",
    "firebase": "^7.7.0",
    "gsap": "^3.0.5",
    "hammerjs": "^2.0.8",
    "handlebars": "^4.7.2",
    "ios-inner-height": "^1.1.1",
    "lodash": "^4.17.15",
    "ngx-cookie-service": "^2.3.0",
    "progressbar.js": "^1.1.0",
    "rxjs": "6.5.4",
    "time-ago-pipe": "^1.3.2",
    "ts-key-enum": "^2.0.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular/cli": "^8.3.23",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@angularclass/hmr": "^2.1.3",
    "@types/jasmine": "3.5.1",
    "@types/jasminewd2": "^2.0.8",
    "@types/mandrill-api": "^1.0.30",
    "@types/node": "^13.1.8",
    "codelyzer": "^5.2.1",
    "jasmine-bamboo-reporter": "0.0.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.1",
    "karma-junit-reporter": "^2.0.1",
    "protractor": "~5.4.2",
    "ts-node": "~8.6.2",
    "tslint": "^6.0.0",
    "typescript": "^3.5.2"
  }
}
