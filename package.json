{
  "name": "mdot",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "dev": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --host local.permanent.org --disable-host-check --configuration=hmr",
    "dev_proxy": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --host local.permanent.org --disable-host-check --configuration=proxy_hmr",
    "dev_local": "ng serve --base-href /",
    "build": "ng build --aot --build-optimizer --prod",
    "build_local": "ng build --aot --configuration=local",
    "build_dev": "ng build --aot --build-optimizer --configuration=development",
    "build_staging": "ng build --aot --build-optimizer --configuration=staging",
    "test": "ng test --source-map=false",
    "test_build": "ng test --source-map=false --watch=false --browsers chrome_headless_no_sandbox",
    "lint": "ng lint",
    "e2e_local": "protractor e2e/protractor.conf.js",
    "e2e_dev": "protractor e2e/protractor.dev.conf.js",
    "e2e_proxy": "ng e2e -c proxy --protractor-config e2e/protractor.local_build.conf.js --host local.permanent.org"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.16",
    "@angular/common": "^7.2.16",
    "@angular/compiler": "^7.2.16",
    "@angular/core": "^7.2.16",
    "@angular/fire": "^5.3.0",
    "@angular/forms": "^7.2.16",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "^7.2.16",
    "@angular/router": "^7.2.16",
    "@ng-bootstrap/ng-bootstrap": "^4.2.2",
    "@types/hammerjs": "^2.0.36",
    "@types/lodash": "^4.14.149",
    "binaryjs": "^0.2.1",
    "core-js": "^2.6.11",
    "countup.js-angular2": "^6.1.1",
    "firebase": "^5.11.1",
    "gsap": "^2.1.3",
    "hammerjs": "^2.0.8",
    "ios-inner-height": "^1.1.1",
    "lodash": "^4.17.15",
    "ngx-cookie-service": "^1.0.10",
    "progressbar.js": "^1.1.0",
    "rxjs": "6.4.0",
    "time-ago-pipe": "^1.3.2",
    "ts-key-enum": "^2.0.2",
    "vis-timeline": "^6.4.3",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.7.5",
    "@angular/cli": "^8.3.23",
    "@angular/compiler-cli": "^7.2.16",
    "@angular/language-service": "^7.2.16",
    "@angularclass/hmr": "^2.1.3",
    "@types/gsap": "^1.20.2",
    "@types/jasmine": "2.8.6",
    "@types/jasminewd2": "^2.0.8",
    "@types/mandrill-api": "^1.0.30",
    "@types/node": "^8.10.59",
    "codelyzer": "^5.2.1",
    "jasmine-bamboo-reporter": "0.0.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "^5.0.0",
    "typescript": "~3.6"
  }
}
