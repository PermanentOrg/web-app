{
  "name": "mdot",
  "version": "2.13.0",
  "scripts": {
    "ng": "ng",
    "serve": "ng serve",
    "dev": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --host local.permanent.org --disable-host-check --configuration=hmr",
    "dev_proxy": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --host local.permanent.org --disable-host-check --configuration=proxy",
    "dev_proxy_hmr": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --host local.permanent.org --disable-host-check --configuration=proxy_hmr",
    "dev_proxy_prod": "node --max_old_space_size=8048 ./node_modules/@angular/cli/bin/ng serve --host local.permanent.org --disable-host-check --configuration=proxy_hmr_prod",
    "dev_local": "ng serve --base-href /",
    "build": "ng build --aot --build-optimizer --prod",
    "build_local": "ng build --aot --configuration=local",
    "build_dev": "ng build --aot --build-optimizer --configuration=development",
    "build_staging": "ng build --aot --build-optimizer --configuration=staging",
    "build_ots": "ng build --aot --build-optimizer --configuration=ots_test",
    "test": "ng test --source-map=false",
    "test_build": "ng test --source-map=false --watch=false --browsers chrome_headless_no_sandbox",
    "lint": "ng lint",
    "e2e_local": "protractor e2e/protractor.conf.js",
    "e2e_dev": "protractor e2e/protractor.dev.conf.js",
    "e2e_proxy": "ng e2e -c proxy --protractor-config e2e/protractor.local_build.conf.js --host local.permanent.org",
    "cypress": "cypress run --config-file cypress.json --browser chrome --headless",
    "cypress_dev": "cypress run --config-file cypress.dev.json --browser chrome --headless",
    "cypress_staging": "cypress run --config-file cypress.staging.json --browser chrome --headless",
    "cypress_prod": "cypress run --config-file cypress.prod.json --browser chrome --headless"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "9.1.11",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "9.1.11",
    "@angular/compiler": "9.1.11",
    "@angular/core": "9.1.11",
    "@angular/fire": "^5.3.0",
    "@angular/forms": "9.1.11",
    "@angular/google-maps": "9.2.4",
    "@angular/localize": "9.1.11",
    "@angular/platform-browser": "9.1.11",
    "@angular/platform-browser-dynamic": "9.1.11",
    "@angular/router": "9.1.11",
    "@ng-bootstrap/ng-bootstrap": "5.3.1",
    "@permanent.org/vis-timeline": "^1.0.3",
    "@sentry/browser": "5.18.0",
    "@types/debug": "4.1.5",
    "@types/hammerjs": "2.0.36",
    "@types/lodash": "4.14.149",
    "binaryjs": "0.2.1",
    "core-js": "3.6.4",
    "countup.js-angular2": "7.3.0",
    "date-fns": "^2.16.1",
    "debug": "4.1.1",
    "firebase": "^7.7.0",
    "firebase-functions": "^3.8.0",
    "fuse.js": "^5.2.3",
    "gsap": "^3.0.5",
    "hammerjs": "^2.0.8",
    "handlebars": "^4.7.2",
    "iconfonts": "^0.12.0",
    "ionicons": "4.6.3",
    "ios-inner-height": "^1.1.1",
    "lodash": "^4.17.15",
    "ngx-bootstrap": "^5.6.1",
    "ngx-cookie-service": "^3.0.4",
    "ngx-route-history": "0.0.4",
    "progressbar.js": "1.0.1",
    "rxjs": "6.5.4",
    "time-ago-pipe": "^1.3.2",
    "ts-key-enum": "^2.0.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.9",
    "@angular/cli": "9.1.9",
    "@angular/compiler-cli": "9.1.11",
    "@angular/language-service": "9.1.11",
    "@angularclass/hmr": "2.1.3",
    "@types/jasmine": "3.5.1",
    "@types/jasminewd2": "^2.0.8",
    "@types/mandrill-api": "^1.0.30",
    "@types/node": "^13.1.8",
    "codelyzer": "^5.2.1",
    "cypress": "^4.11.0",
    "cypress-promise": "^1.1.0",
    "jasmine-bamboo-reporter": "0.0.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.1",
    "karma-junit-reporter": "^2.0.1",
    "karma-spec-reporter": "0.0.32",
    "karma-verbose-reporter": "0.0.6",
    "mailslurp-client": "^8.7.8",
    "protractor": "~5.4.2",
    "ts-node": "~8.6.2",
    "tslib": "^1.11.1",
    "tslint": "^6.0.0",
    "typescript": "3.7.5"
  }
}
