{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mdot": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "pr",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/mdot",
            "index": "src/index.html",
            "main": "src/main.ts",
            "showCircularDependencies": false,
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/@permanent.org/vis-timeline/dist/vis-timeline-graph2d.css",
              "src/styles/main.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles"
              ]
            },
            "scripts": [
              "src/vendor/binary.js",
              "node_modules/ios-inner-height/dist/ios-inner-height.min.js",
              "node_modules/gsap/dist/gsap.js"
            ]
          },
          "configurations": {
            "local": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": {
                "hidden": false,
                "scripts": true,
                "styles": true,
                "vendor": false
              },
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "deployUrl": "/app/"
            },
            "proxy_dev": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.proxy.ts"
                }
              ],
              "deployUrl": "/",
              "tsConfig": "src/tsconfig.serve.json"
            },
            "proxy_local": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.local.proxy.ts"
                }
              ],
              "deployUrl": "/",
              "tsConfig": "src/tsconfig.serve.json"
            },
            "development": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ],
              "index": "src/index/dev/index.html",
              "deployUrl": "/app/",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": {
                "hidden": true,
                "scripts": true,
                "styles": true,
                "vendor": false
              },
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "index": "src/index/prod/index.html",
              "deployUrl": "/app/",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": {
                "hidden": true,
                "scripts": true,
                "styles": true,
                "vendor": false
              },
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ],
              "index": "src/index/staging/index.html",
              "deployUrl": "/app/",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": {
                "hidden": true,
                "scripts": true,
                "styles": true
              },
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "mdot:build"
          },
          "configurations": {
            "dev": {
              "host": "local.permanent.org",
              "hmr": false,
              "ssl": true,
              "sslCert": "/etc/ssl/STAR_permanent_org.crt",
              "sslKey": "/etc/ssl/permanent.key",
              "browserTarget": "mdot:build:proxy_dev",
              "deployUrl": "/",
              "baseHref": "/",
              "proxyConfig": "proxy.conf.dev.json"
            },
            "local": {
              "host": "ng.permanent.org",
              "hmr": false,
              "ssl": true,
              "sslCert": "/etc/ssl/STAR_permanent_org.crt",
              "sslKey": "/etc/ssl/permanent.key",
              "browserTarget": "mdot:build:proxy_local",
              "deployUrl": "/",
              "baseHref": "/",
              "proxyConfig": "proxy.conf.local.json"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "mdot:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles/main.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles"
              ]
            },
            "scripts": [
              "src/vendor/binary.js",
              "node_modules/gsap/dist/gsap.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "mdot-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "mdot:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "mdot:serve:production"
            },
            "proxy": {
              "devServerTarget": "mdot:serve:proxy"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "mdot",
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    }
  },
  "cli": {
    "analytics": "0f7fbf7e-c8eb-4198-b099-2c1d5308c974"
  }
}
