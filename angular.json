{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "mdot": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "pr",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/mdot",
              "browser": ""
            },
            "index": "src/index.html",
            "polyfills": [
              "src/polyfills.ts",
              "@angular/localize/init"
            ],
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "node_modules/vis-timeline/dist/vis-timeline-graph2d.css",
              "src/styles/main.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles"
              ]
            },
            "scripts": [
              "node_modules/ios-inner-height/dist/ios-inner-height.min.js",
              "node_modules/gsap/dist/gsap.js"
            ],
            "externalDependencies": [
              "assets/*"
            ],
            "aot": false,
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "browser": "src/main.ts"
          },
          "configurations": {
            "local": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": {
                "hidden": false,
                "scripts": true,
                "styles": true,
                "vendor": false
              },
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "deployUrl": "/app/"
            },
            "proxy_local": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.local.proxy.ts"
                }
              ],
              "deployUrl": "/",
              "baseHref": "/",
              "tsConfig": "src/tsconfig.serve.json"
            },
            "proxy_local-docker": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.local-docker.proxy.ts"
                }
              ],
              "deployUrl": "/",
              "baseHref": "/",
              "tsConfig": "src/tsconfig.serve.json"
            },
            "development": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.dev.ts"
                }
              ],
              "index": "src/index/dev/index.html",
              "deployUrl": "/app/",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": {
                "hidden": true,
                "scripts": true,
                "styles": true,
                "vendor": false
              },
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "index": "src/index/prod/index.html",
              "deployUrl": "/app/",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": {
                "hidden": true,
                "scripts": true,
                "styles": true,
                "vendor": false
              },
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true
            },
            "staging": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.staging.ts"
                }
              ],
              "index": "src/index/staging/index.html",
              "deployUrl": "/app/",
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": {
                "hidden": true,
                "scripts": true,
                "styles": true
              },
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "mdot:build"
          },
          "configurations": {
            "local": {
              "host": "ng.permanent.org",
              "hmr": false,
              "ssl": true,
              "proxyConfig": "proxy.conf.local.json",
              "buildTarget": "mdot:build:proxy_local"
            },
            "local-docker": {
              "host": "local.permanent.org",
              "hmr": false,
              "ssl": true,
              "proxyConfig": "proxy.conf.local.json",
              "buildTarget": "mdot:build:proxy_local-docker"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "mdot:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "sourceMap": true,
            "styles": [
              "src/styles/main.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles"
              ]
            },
            "scripts": [
              "node_modules/gsap/dist/gsap.js"
            ],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "src/**/*.ts",
              "src/**/*.html"
            ]
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": ".storybook",
            "browserTarget": "mdot:build-storybook",
            "compodoc": false,
            "port": 6006,
						"styles": ["src/styles/main.scss"],
            "stylePreprocessorOptions": {
              "includePaths": [
                "src/styles"
              ]
            },
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
					"options": {
						"configDir": ".storybook",
						"browserTarget": "mdot:build",
						"outputDir": "storybook-static",
						"compodoc": false,
						"styles": ["src/styles/main.scss"],
						"stylePreprocessorOptions": {
							"includePaths": ["src/styles"]
						}
					}
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:component": {
      "style": "scss"
    },
    "@angular-eslint/schematics:application": {
      "setParserOptionsProject": true
    },
    "@angular-eslint/schematics:library": {
      "setParserOptionsProject": true
    }
  },
  "cli": {
    "analytics": false
  }
}
