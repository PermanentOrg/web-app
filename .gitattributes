# Autodetect text files
* text=lf

# Force the following filetypes to have unix eols, so Windows does not break them
*.php text eol=lf
*.html text eol=lf
*.txt text eol=lf
*.js text eol=lf
*.json text eol=lf
*.css text eol=lf
*.xml text eol=lf
*.bash text eol=lf
*.sh text eol=lf
*.bat text eol=lf

# Force images/fonts to be handled as binaries
*.jpg binary
*.jpeg binary
*.gif binary
*.png binary
*.t3x binary
*.t3d binary
*.exe binary
*.data binary
*.ttf binary
*.eof binary
*.eot binary
*.swf binary
*.mov binary
*.mp4 binary
*.mp3 binary
*.ogg binary
*.flv binary
*.zip binary
*.woff binary
*.mo binary
*.otf binary
*.gz binary
*.svg binary
*.mp4 binary
*.java binary
*.7z binary
*.aac binary
*.m4a binary
*.mp3 binary
*.wma binary
*.docx binary
*.doc binary
*.xls binary
*.xlsx binary
*.pdf binary
*.otp binary
*.pdt binary
*.ots binary
*.ged binary
*.webp binary
*.webm binary
*.mov binary
*.avi binary
*.ai binary
*.tar binary
*.ico binary
*.pbm binary
*.phar binary
